{
  "input_res_h": 640,
  "input_res_v": 480,
  "input_fps": 25,
  "debug_level": 2,
  "streaming_host": "brix.local",
  "streaming_port": 5000,
  "fast_area_size": 230,
  "-stream_cmd": [
    "cvlc",
    "--demux=rawvideo",
    "--rawvid-fps=25",
    "--rawvid-width=1280",
    "--rawvid-height=720",
    "--rawvid-chroma=RV24",
    "-",
    "--sout",
    "#transcode{vcodec=h264,crf=15,fps=25}:std{access=udp,mux=ts,dst=192.168.1.126:1234}"
  ],
  "1stream_cmd": [
    "cvlc",
    "-I dummy",
    "--demux=rawvideo",
    "--rawvid-fps=25",
    "--rawvid-width=640",
    "--rawvid-height=480",
    "--rawvid-chroma=RV24",
    "-",
    "--sout",
    "#transcode{vcodec=h264,crf=15,fps=25}:std{access=file,mux=mp4,dst=debug.mp4}"
  ],
  "zzstream_cmd": [
    "ffmpeg",
    "-y",
    "-pix_fmt",
    "bgr24",
    "-f",
    "rawvideo",
    "-s",
    "640x480",
    "-r",
    "25",
    "-i",
    "-",
    "-crf",
    "15",
    "-vcodec",
    "h264_omx",
    "debug.mp4"
  ],
  "xxstream_cmd": [
    "gst-launch-1.0",
    "-v",
    "fdsrc",
    "!",
    "video/x-raw,format=I420,width=640,height=480,framerate=25/1",
    "!",
    "queue",
    "!",
    "videoconvert",
    "!",
    "video/x-raw,format=I420",
    "!",
    "omxh264enc",
    "!",
    "mpegtsmux",
    "!",
    "udpsink",
    "host=brix.local",
    "port=5000"
  ],
  "gstreamer_pipe": [
    "appsrc is-live=true do-timestamp=true",
    "videoconvert",
    "video/x-raw,format=I420,width={width},height={height},framerate={fps}/1",
    "omxh264enc control-rate=1 target-bitrate=4000000",
    "mpegtsmux",
    "udpsink host={host} port={port}"
  ]
}